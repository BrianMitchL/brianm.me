{
  "name": "brianmitchl.github.io",
  "version": "1.0.0",
  "private": true,
  "description": "Website for Brian Mitchell",
  "scripts": {
    "prettier": "prettier --write '**/*.{js,scss,css,json,md,yml}'",
    "start": "run-p start:js start:11ty",
    "prebuild": "rimraf _site",
    "build": "run-s build:js build:11ty",
    "start:js": "rollup -c -w",
    "build:js": "rollup -c",
    "start:11ty": "ELEVENTY_ENV=development eleventy --serve",
    "build:11ty": "ELEVENTY_ENV=production eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianMitchL/BrianMitchL.github.io.git"
  },
  "author": "Brian Mitchell",
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "@quasibit/eleventy-plugin-schema": "^1.0.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.0.3",
    "eleventy-plugin-emoji": "^1.0.0",
    "eleventy-plugin-excerpt": "^1.1.2",
    "eleventy-plugin-sitemap": "^1.0.1",
    "luxon": "^1.25.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-table-of-contents": "^0.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.27.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.25.1"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
