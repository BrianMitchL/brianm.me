{
  "name": "brianmitchl.github.io",
  "version": "1.0.0",
  "private": true,
  "description": "Website for Brian Mitchell",
  "scripts": {
    "prettier": "prettier --write '**/*.{js,mjs,pcss,css,json,md,yml}'",
    "start": "run-p start:js start:css start:11ty",
    "prebuild": "rimraf _site",
    "build": "run-s build:js build:css build:11ty",
    "start:css": "postcss src/assets/css/style.pcss -o src/assets/css/style.css -w",
    "build:css": "postcss src/assets/css/style.pcss -o src/assets/css/style.css --env production",
    "start:js": "rollup -c -w",
    "build:js": "rollup -c",
    "start:11ty": "eleventy --serve",
    "build:11ty": "eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianMitchL/BrianMitchL.github.io.git"
  },
  "author": "Brian Mitchell",
  "dependencies": {
    "@exampledev/new.css": "^1.1.3",
    "csshake": "^1.5.3",
    "svelte": "^3.31.2",
    "typeface-inter": "^3.15.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "@quasibit/eleventy-plugin-schema": "^1.0.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.0.4",
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "autoprefixer": "^10.2.3",
    "cssnano": "^4.1.10",
    "eleventy-plugin-emoji": "^1.0.0",
    "eleventy-plugin-excerpt": "^1.1.2",
    "eleventy-plugin-sitemap": "^1.0.1",
    "luxon": "^1.25.0",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-table-of-contents": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-nesting": "^7.0.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.0",
    "rollup-plugin-filesize": "^9.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
