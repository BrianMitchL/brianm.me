{
  "name": "brianmitchl.github.io",
  "version": "1.0.0",
  "private": true,
  "description": "Website for Brian Mitchell",
  "scripts": {
    "prettier": "prettier --write '**/*.{js,mjs,pcss,css,json,md,yml}'",
    "start": "TZ=America/Chicago run-p start:js start:css start:11ty",
    "prebuild": "rimraf _site",
    "build": "TZ=America/Chicago run-s build:js build:css build:11ty",
    "start:css": "postcss src/assets/css/*.pcss -d src/assets/css --ext css -w",
    "build:css": "postcss src/assets/css/*.pcss -d src/assets/css --ext css --env production",
    "start:js": "rollup -c -w",
    "build:js": "rollup -c",
    "start:11ty": "eleventy --serve",
    "build:11ty": "eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianMitchL/BrianMitchL.github.io.git"
  },
  "author": "Brian Mitchell",
  "dependencies": {
    "@fontsource/inter": "^4.5.1",
    "@primer/octicons": "^16.2.0",
    "canvas-confetti": "^1.4.0",
    "csshake": "^1.5.3",
    "prism-themes": "^1.9.0",
    "svelte": "^3.46.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "@quasibit/eleventy-plugin-schema": "^1.4.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.4",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@sindresorhus/slugify": "^1.1.2",
    "@types/markdown-it": "^12.2.3",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.15",
    "eleventy-plugin-emoji": "^1.1.0",
    "eleventy-plugin-excerpt": "^1.1.2",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "luxon": "^2.3.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-toc-done-right": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^10.1.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.64.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sharp": "^0.29.3",
    "terser": "^5.10.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ]
}
